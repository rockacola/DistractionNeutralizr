{
  "name": "Distraction Neutralizr",
  "description": "Google Chrome extension to actively remove 'distraction contents' from the web page.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "version": "0.0.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["src/bg/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Distraction Neutralizr - The Title"
  },
  "manifest_version": 2
}
